{
    "name": "snippeppa",
    "publisher": "MatheusPereira",
    "displayName": "Snippeppa",
    "description": "Pacote com snippets simples que facilitam o desenvolvimento e debugs",
    "author": "Matheus Pereira (https://github.com/matmper)",
    "icon": "images/icon.jpg",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.52.0"
    },
    "keywords": [
        "SNIPPETS",
        "PHP",
        "JS",
        "TWIG"
    ],
    "categories": [
        "Snippets"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/matmper/vscode-snippets.git"
    },
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./snippets/php.json"
            },
            {
                "language": "js",
                "path": "./snippets/js.json"
            },
            {
                "language": "twig",
                "path": "./snippets/twig.json"
            }
        ]
    }
}
